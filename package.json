{
  "name": "netshop",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "createProxyJson": "node createProxyJson.js",
    "runTest": "set GLOABAL_ENV=test && npm run createProxyJson && ng serve --proxy-config proxy.conf.json",
    "runDev": "set GLOABAL_ENV=dev && npm run createProxyJson && ng serve --proxy-config proxy.conf.json",
    "runProd": "set GLOABAL_ENV=prod && npm run createProxyJson && ng serve --proxy-config proxy.conf.json",
    "buildAndWar": "ng build --base-href /netshop-fe/ && gulp build-war",
    "buildAndWarProd": "ng build --prod --base-href /netshop-fe/ && gulp build-war",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.0",
    "@angular/cdk": "^7.2.1",
    "@angular/common": "^7.2.0",
